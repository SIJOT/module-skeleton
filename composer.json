{
  "name": ":author_name/:package_name", 
  "description": ":package_description", 
  "keywords": [
    ":author_name", 
    ":package_name"
  ], 
  "homepage": "https://github.com/:author_name/:package_name", 
  "license": "MIT", 
  "authors": [
    {
      "name"= ":author_name", 
      "email"= ":author_email", 
      "homepage"= ":author_website", 
      "role:" "developer"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  }
  "require-dev": {
    "phpunit/phpunit": "4.*"
  }
  "autoload" {
    "psr-4": {
      ":author_name\\:package_name\\": "src"
    }
  },
  "autoload-dev" {
    "psr-4": {
      ":author_name\\:package_name\\Test\\": "tests"
    }
  }
  "scripts": {
    "test": "phpunit"
  }
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.dev"
    }
  }
}
